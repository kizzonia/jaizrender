@import "bootstrap";
@import url('https://fonts.googleapis.com/css?family=PT+Serif:400,400i,700,700i|Poppins:300,400,500,600,700i,900|Montserrat:300,400,500,600,700&amp;display=swap');
@import "flaticon.css";

 @import url("https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css");
 @import url('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.0-2/css/all.min.css');
 @import url('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css');

@import "fontawesome-all";
@import "animate";
@import "jquery-ui";
@import "owl";
@import "jquery.bootstrap-touchspin";
@import "jquery.mCustomScrollbar.min";
@import "jquery.fancybox.min";
@import "style.scss.erb";

@import "responsive";
@import "color-themes/default-theme";

@font-face {
	font-family: 'Flaticon';
	src:url('<%= asset_path ('Flaticon.eot') %>');
	src:url('<%= asset_path ('Flaticond41d.eot') %>') format('embedded-opentype'),
		url(' <%= asset_path ('Flaticon.woff2') %>') format('woff2'),
		url(' <%= asset_path ('Flaticon.woff') %>') format('woff'),
		url(' <%= asset_path ('Flaticon.ttf') %>') format('truetype'),
		url('<%= asset_path ('Flaticon.svg') %>') format('svg');
	font-weight: normal;
	font-style: normal;
};
